{
	"_version": "1.12.0",
	"sap.app": {
		"id": "br.com.switch.farmtenz",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "br.com.switch.farmtenz.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.120.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"handleValidation": true,
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "br.com.switch.farmtenz.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "br.com.switch.farmtenz.view",
				"controlId": "app",
				"bypassed": {
					"target": "NotFound"
				},
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "login",
					"target": "login"
				},
				{
					"pattern": "register",
					"name": "register",
					"target": "register"
				},
				{
					"name": "NotFound",
					"pattern": "NotFound",
					"target": "NotFound"
				},
				{
					"pattern": "{token}/home",
					"name": "home",
					"target": "home"
				}
			],
			"targets": {
				"login": {
					"viewId": "Login",
					"transition": "show",
					"viewName": "loginView/Login"
				},
				"register": {
					"viewId": "Register",
					"transition": "show",
					"viewName": "registerView/Register"
				},
				"NotFound": {
					"viewId": "NotFound",
					"viewName": "NotFound",
					"transition": "show",
					"clearControlAggregation": false
				},
				"home": {
					"viewId": "Home",
					"viewName": "homeView/Home",
					"transition": "slide",
					"clearControlAggregation": false
				}
			}
		}
	},
  
	"name": "Farmtenz",
	"short_name": "Farmtenz",
	"start_url": "/",
	"display": "standalone",
	"background_color": "#ffffff",
	"theme_color": "#000000",
	"icons": [
		{
			"src": "img/farmacia.png",
			"sizes": "192x192",
			"type": "image/png"
		}
	]
}
