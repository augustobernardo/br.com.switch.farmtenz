<mvc:View controllerName="br.com.switch.salestem.controller.Home" height="100%"
  xmlns="sap.m"
  xmlns:tnt="sap.tnt"
  xmlns:core="sap.ui.core"
  xmlns:mvc="sap.ui.core.mvc">

  <tnt:ToolPage id="toolPageId">

    <tnt:header>
      <tnt:ToolHeader id="toolHeader">
        <core:Fragment fragmentName="br.com.switch.salestem.view.fragments.HeaderBar" type="XML" />
      </tnt:ToolHeader>
    </tnt:header>

    <tnt:sideContent >
      <core:Fragment fragmentName="br.com.switch.salestem.view.fragments.SideBar" type="XML" />
    </tnt:sideContent>

    <tnt:mainContents>
      <NavContainer id="page_content_entrada_saida_comp" visible="{homeView>/Pages/VisibleInOutComp}">
        <core:Fragment fragmentName="br.com.switch.salestem.view.fragments.InputOutputComp" type="XML" />
      </NavContainer>

      <NavContainer id="page_content_comp_quimicos" visible="{homeView>/Pages/VisibleCompQuimicos}">
        <core:Fragment fragmentName="br.com.switch.salestem.view.fragments.CompQuimicos" type="XML" />
      </NavContainer>

    </tnt:mainContents>

  </tnt:ToolPage>
</mvc:View>
